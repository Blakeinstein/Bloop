{
  "name": "bloop",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "serve": "cross-env env=DEBUG parcel src/web/index.html --port 8080",
    "build": "cross-env env=PROD parcel build src/web/index.html --target browser --out-dir dist/ --no-content-hash",
    "tauri-serve": "cargo run --no-default-features",
    "dev": "npm-run-all --parallel serve tauri-serve",
    "make": "yarn build && cargo build",
    "release": "yarn make --release"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-beta.1",
    "codemirror": "^5.55.0",
    "fuse.js": "^6.4.0"
  },
  "devDependencies": {
    "@kenpb/parcel-plugin-ifdef": "^1.1.2",
    "@tauri-apps/cli": "^1.0.0-beta.2",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "parcel-plugin-inline-source": "^1.0.0"
  }
}
