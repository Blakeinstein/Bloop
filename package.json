{
  "name": "bloop",
  "version": "0.1.0",
  "main": "index.html",
  "license": "MIT",
  "targets": {
    "main": {
      "context": "browser"
    }
  },
  "scripts": {
    "serve": "parcel src/web/index.html  --dist-dir dist/ --port 8080",
    "build": "parcel build src/web/index.html --dist-dir dist/ --no-content-hash",
    "tauri-serve": "cargo run --no-default-features",
    "dev": "npm-run-all --parallel serve tauri-serve",
    "make": "yarn build && cargo build",
    "release": "yarn make --release"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.0.0-beta.1",
    "codemirror": "^5.61.0",
    "fuse.js": "^6.4.6",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0-beta.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-beta.3.1"
  }
}
